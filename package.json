{
  "name": "gemini-proxy",
  "version": "1.0.1",
  "description": "Gemini API proxy service with load balancing and key management",
  "main": "dist/main.js",
  "bin": {
    "gemini-proxy": "./dist/main.js"
  },
  "scripts": {
    "build": "tsc && tsc-alias && chmod +x dist/main.js",
    "dev": "nodemon",
    "start": "node dist/main.js",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.ts",
    "db:migrate": "ts-node -r tsconfig-paths/register src/database/migrate.ts migrate",
    "db:migrate:prod": "ts-node -r tsconfig-paths/register src/database/migrate.ts migrate --prod",
    "db:migrate:create": "ts-node -r tsconfig-paths/register src/database/migrate.ts create",
    "db:reset": "ts-node -r tsconfig-paths/register src/database/migrate.ts reset",
    "db:validate": "ts-node -r tsconfig-paths/register src/database/migrate.ts validate",
    "db:push": "npx prisma db push",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"    
  },
  "keywords": [
    "gemini",
    "proxy",
    "fastify",
    "load-balancing"
  ],
  "author": "Terrence",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TerrenceMiao/gemini-proxy.git"
  },
  "homepage": "https://github.com/TerrenceMiao/gemini-proxy#README",
  "bugs": {
    "url": "https://github.com/TerrenceMiao/gemini-proxy/issues"
  },  
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/multipart": "^9.0.3",
    "@prisma/client": "^6.13.0",
    "axios": "^1.11.0",
    "dotenv": "^17.2.1",
    "fastify": "^5.4.0",
    "google-auth-library": "^10.2.1",
    "mysql2": "^3.14.3",
    "node-cron": "^4.2.1",
    "openai": "^5.12.1",
    "pino": "^9.8.0",
    "pino-pretty": "^13.1.1",
    "prisma": "^6.13.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.0",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}